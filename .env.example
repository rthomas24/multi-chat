# Domain configuration (for production CORS)
NEXT_PUBLIC_DOMAIN=https://your-domain.com

# Encryption salt for API keys (change this to a random string)
NEXT_PUBLIC_ENCRYPTION_SALT=your-random-encryption-salt

# Optional: Default API keys (not recommended for production)
# These are only used as fallbacks if user doesn't provide their own keys
# OPENAI_API_KEY=
# ANTHROPIC_API_KEY=
# GOOGLE_API_KEY=
# XAI_API_KEY= 